        'delete_foo_bar' => array(
            'title' => t('Delete entity content.'),
        ),
        'add_foo_bar' => array(
            'title' => t('Add entity content'),
        ),
        'view_foo_bar' => array(
            'title' => t('View entity content'),
        ),
        'edit_foo_bar' => array(
            'title' => t('Edit entity content'),
        ),
        'admin_foo_bar' => array(
            'title' => t('Administer settings'),
        ),
    );
}
